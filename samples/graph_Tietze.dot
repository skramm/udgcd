# see https://en.wikipedia.org/wiki/Tietze%27s_graph


graph G {


# inner triangle
0 [pos="-1,0!"];
1 [pos="+1,0!"];
2 [pos="0,1.5!"];
0--1;
1--2;
2--0;


# outside perimeter


4 [pos="+1.5,-3!"];
5 [pos="-1.5,-3!"];

3--4;
4--5;
5--6;
6--7;
7--8;
8--9;
9--10;
10--11;
3--11;

9 [pos="0,4!"];

6 [pos="-4,-1!"];
3 [pos="+4,-1!"];

1--3;
0--6;
2--9;

8 [pos="-3,3!"];
10 [pos="+3,3!"];

7 [pos="-4,2!"];
11 [pos="+4,2!"];

4--8;
5--10;
7--11;
}

